<Application x:Class="PaperDownloader.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PaperDownloader"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ResourceDictionaries/Progressbar.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/Window.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/Buttons.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/DropDown.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/Grid.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/TextBox.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
